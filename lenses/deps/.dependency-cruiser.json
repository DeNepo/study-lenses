{
  "outputType": "dot",
  "doNotFollow": {
    "path": ["node_modules"]
  },
  "reporterOptions": {
    "dot": {
      "collapsePattern": "^((node_modules)/[^/]+)",
      "orderBy": "name",
      "theme": {
        "edge": {
          "arrowsize": "0.5",
          "penwidth": "1.0",
          "color": "darkgrey"
        },
        "graph": {
          "splines": "ortho"
        }
      }
    }
  }
}
